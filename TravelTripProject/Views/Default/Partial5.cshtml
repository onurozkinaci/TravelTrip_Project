@using TravelTripProject.Models.Classes
@model List<Blog>

<div class="box1-left">
    @foreach (var item in Model)
    {
        <div class="bot-img">
            <img src="@item.BlogImage" alt="" style="height:70px;width:120px;"/>
        </div>
        <div class="bot-desc1">
            <p>@item.Description.Substring(0, 65)...</p>
        </div>
        <div class="clear"></div>
    }
</div>
